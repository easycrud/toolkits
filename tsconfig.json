{
  "compilerOptions": {
    "moduleResolution": "node",
    "target": "esnext",
    "rootDir": ".",
    "outDir": ".",
    "declaration": true,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "composite": true
  },
  "files": [
    "package.json",
  ]
}
